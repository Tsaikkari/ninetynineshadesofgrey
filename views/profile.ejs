<%- include("header") -%>
<div class="container" id="profile-screen" style="background: #fff">
  <div class="row">
    <div class="picture col-lg-6">
      <img src="">  
    </div>
    <div class="posts col-lg-6">
      <% posts.forEach(function(post){ %>
        <h1 class="person-name"> <%= post.personName %> </h1>
        <p class="description"> <%= post.description %> </p>
      <% }) %>
    </div>
  </div>
</div>

